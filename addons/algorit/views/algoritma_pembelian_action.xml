<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <record id="pembelian_actions_id" model="ir.action.act_window">
        <field name="name">Pembelian</field>
        <field name="res_model">algoritma.pembelian</field>
    </record>

    <record id="pembelian_actions_tree_view_id" model="ir.action.act_window.view">
        <field name="sequance" eval="1" />
        <field name="view_mode">tree</field>
        <field name="view_id" ref="algoritma_pembelian.algoritma_pembelian_tree_view_id" />
        <field name="act_window_id" ref="algoritma_pembelian.pembelian_action.id" />
    </record>

    <record id="pembelian_actions_form_view_id" model="ir.action.act_window.view">
        <field name="sequance" eval="2" />
        <field name="view_mode">form</field>
        <field name="view_id" ref="algoritma_pembelian.algoritma_pembelian_form_view_id" />
        <field name="act_window_id" ref="algoritma_pembelian.pembelian_action.id" />
    </record>
</odoo>